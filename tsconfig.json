{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "ES2020",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts",
  "**/*.ts",
  "**/*.tsx",
  ".next/types/**/*.ts",
  "app/**/*.ts",
  "app/**/*.js",
  "server.js", "components/ui/HeroSection.tsx", "components/ui/pricing.tsx", "components/ui/footers.tsx", "components/ui/waitlist.tsx", "components/ui/testimonial.jsx", "app/dashboard/snippets/dashboard.jsx", "app/dashboard/developer/tabs/apikey.jsx", "app/controllers/apikeyscontroller.js"] ,
  "exclude": ["node_modules"]
}
